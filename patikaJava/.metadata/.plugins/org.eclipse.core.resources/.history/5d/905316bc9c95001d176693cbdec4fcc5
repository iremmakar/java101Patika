package palindromKelime;

public class Main {

	public static void main(String[] args) {
		String x = "abba";
		int count=0;
		int uzunluk = x.length();
		
		for(int i =0;i<uzunluk/2;i++) {
			if(x.charAt(i)==x.charAt(uzunluk-1)) {
				count++;
			}
		}
		
		if(count==uzunluk/2) {
			System.out.println("palindorm");
		}
		
		

	}

}
