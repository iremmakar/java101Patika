package bHarfiStar;

public class Main {

	public static void main(String[] args) {
		String[][] arr = new String[7][4];
		
		
		for(int x=0;x<arr.length;x++) {
			for(int y=0;y<arr[x].length;y++) {
				if(x==0 || x==3 || x==6) {
					arr[x][y]="*";
				}else if((x==1 || x==2 || x==4 || x==5 ) && (y==3 || y==0) {
					arr[x][y]="*";
				}else {
					arr[x][y]=" ";
				}
			}
		}
		
		
		for(int x=0;x<arr.length;x++) {
			for(int y=0;y<arr[x].length;y++) {
		System.out.print(arr[x][y]);
			}
			System.out.println();
		}
	}

}
